{
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            "."
        ],
        "exclude": [
            "node_modules",
            "docs"
        ]
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}